{
  "kind": "collectionType",
  "collectionName": "registrations",
  "info": {
    "singularName": "registration",
    "pluralName": "registrations",
    "displayName": "Registration"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "emailAddress": {
      "type": "email",
      "required": true
    },
    "phoneNumber": {
      "type": "string"
    },
    "ticketCode": {
      "type": "string"
    },
    "attendanceType": {
      "type": "string"
    },
    "eventTitle": {
      "type": "string"
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "registrations"
    },
    "isCheckedIn": {
      "type": "boolean",
      "default": false
    },
    "attendeePhoto": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
