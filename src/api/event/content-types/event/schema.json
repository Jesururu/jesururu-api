{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Book Launch",
        "Podcast",
        "Speaking",
        "Conference",
        "Worship Night",
        "Movie Premiere"
      ]
    },
    "EventDateTime": {
      "type": "datetime"
    },
    "Venue": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Poster": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "RegistrationLInk": {
      "type": "string"
    },
    "WhatsAppLink": {
      "type": "string"
    },
    "registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::registration.registration",
      "mappedBy": "event"
    },
    "Team": {
      "type": "component",
      "component": "event.team-member",
      "repeatable": true
    }
  }
}
